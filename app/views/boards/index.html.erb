<p id="notice"><%= notice %></p>

<h1>Listing Boards</h1>

<table>
  <thead>
    <tr>
      <th>Activityname</th>
      <th>Brandname</th>
      <th>Campaignname</th>
      <th>Countryname</th>
      <th>Employeename</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @boards.each do |board| %>
      <tr>
        <td><%= board.activityName %></td>
        <td><%= board.brandName %></td>
        <td><%= board.campaignName %></td>
        <td><%= board.countryName %></td>
        <td><%= board.employeeName %></td>
        <td><%= link_to 'Show', board %></td>
        <td><%= link_to 'Edit', edit_board_path(board) %></td>
        <td><%= link_to 'Destroy', board, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Board', new_board_path %>
