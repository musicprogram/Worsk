  <link rel="stylesheet" href="../../../assets/stylesheets/centerinput.css">

  <div class="col-sm-4">

   <i class="fa fa-user fa-3x logoentrada"></i>
   <br><br>


<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
 
 

  <div class="field">
    <br />
    <%= f.email_field :email, autofocus: true, class: 'form-control centerinput' %>
  </div>



  <div class="field">
    <br />
    <%= f.password_field :password, autocomplete: "off", class: 'form-control centerinput'  %>
  </div>




  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
    






  <% end -%>
<br>
  <div class="actions">
    <%= f.submit "Log in", class: 'btn btn-info ' %>
  </div>
  <br>
<% end %>

  <%= link_to "<i class='fa fa-user-plus fa-2x'></i>".html_safe, new_registration_path(resource_name) %> |
  <%= link_to "<i class='fa fa-user-md fa-2x'></i>".html_safe, new_password_path(resource_name) %>



</div>